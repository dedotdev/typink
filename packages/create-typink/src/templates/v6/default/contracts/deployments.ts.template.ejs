import { ContractDeployment, <%= options.networks %> } from 'typink';

<% if (options.presetContract === 'flipper') { %>
    import flipperMetadata from './artifacts/flipper/flipper.json';
<% } else if (options.presetContract === 'psp22') { %>
    import psp22Metadata from './artifacts/psp22/psp22.json';
<% } %>

export enum ContractId {
  <% if (options.presetContract === 'flipper') { %>
  FLIPPER = 'flipper',
  <% } else if (options.presetContract === 'psp22') { %>
  PSP22 = 'psp22',
  <% } %>
}

<% if (options.presetContract === 'flipper') { %>
export const flipperDeployments: ContractDeployment[] = [
<% if (options.networks.includes("popTestnet")) { %>
  {
    id: ContractId.FLIPPER,
    metadata: flipperMetadata as any,
    network: popTestnet.id,
    address: '0x8036a0ff5670f418FB382cA8a2367A35970C9cdc',
  },
<% } %>
];
<% } else if (options.presetContract === 'psp22') { %>
export const psp22Deployments: ContractDeployment[] = [
<% if (options.networks.includes("popTestnet")) { %>
  {
    id: ContractId.PSP22,
    metadata: psp22Metadata as any,
    network: popTestnet.id,
    address: '0x5fdF0C445145e34E86216FEECB83fe11e170756d',
  },
<% } %>
];
<% } %>

export const deployments = [
  <% if (options.presetContract === 'flipper') { %>
    ...flipperDeployments, 
  <% } else if (options.presetContract === 'psp22') { %>
    ...psp22Deployments
  <% } %>
];
